RewriteEngine  On    # Turn on the rewriting engine
#RewriteBase    /

RewriteRule    ^/?$    articles.php    [NC,L]    # Handle requests for "/"
RewriteRule    ^oversigt/?$    articles.php    [NC,L]    # Handle requests for "oversigt"
RewriteRule    ^logind/?$    login.php    [NC,L]    # Handle requests for "logind"
RewriteRule    ^logud/?$    api/shared/logout.php    [NC,L]    # Handle requests for "logud"
RewriteRule    ^historik/?$    history.php    [NC,L]    # Handle requests for "historik"
RewriteRule    ^statistik/?$    statistic.php    [NC,L]    # Handle requests for "statistik"
RewriteRule    ^projekter/?$    projects.php    [NC,L]    # Handle requests for "projekter"
RewriteRule    ^udlån/?$    loan.php    [NC,L]    # Handle requests for "udlån"
RewriteRule    ^returnering/?$    returning.php    [NC,L]    # Handle requests for "returnering"